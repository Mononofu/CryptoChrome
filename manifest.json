{
  "name": "CryptoChrome",
  "version": "1.1",
  "manifest_version": 2,

  "description": "A GnuPG interface for Chrome",
  "icons": {
    "48": "gnupg.jpg"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs", "contextMenus", "<all_urls>", "clipboardRead", "clipboardWrite"
  ],
  "options_page": "options.html",
  "plugins": [
    { "path": "CryptoChrome.plugin"},
    { "path": "npCryptoChrome.so"}
  ]
}
